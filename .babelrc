{
    "presets": [
      ["env", {"modules": false}],
      "react"
    ],
    "plugins": [
      ["transform-runtime"],
      ["transform-object-rest-spread", { "useBuiltIns": true }],
      ["transform-class-properties"],
      ["transform-runtime"],
      ["syntax-dynamic-import"]
    ],
    "comments": false,
    "env": {
      "test": {
        "presets": ["env"],
        "plugins": [ "istanbul" ]
      }
    }
}